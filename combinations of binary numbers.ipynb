    "#combinations of binary numbers\n",
    "def binary(n,b=''):\n",
    "    if n==0:\n",
    "        print(b)\n",
    "        return\n",
    "    binary(n-1,b+'0')\n",
    "    binary(n-1,b+'1')\n",
    "length=int(input(\"Enter length of string:\"))\n",
    "print(\"Binary combinations....\")\n",
    "binary(length)"
   ]
